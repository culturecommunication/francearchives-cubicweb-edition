{
  "env": {
    "production": {
      "presets": [
          ["@babel/preset-react", {
              "targets": {
                  "browsers": ["Firefox >= 48", "Chrome >= 50", "ie >= 9"]
              }
          }]
      ]
    },
    "development": {
        "presets": [
            ["@babel/preset-react", {
                "targets": {
                    "browsers": ["last 1 Chrome version", "last 1 Firefox version"]
                }
            }]
        ]
    },
    "test": {
      "presets": [
          ["@babel/preset-env", {
              "targets": {
                  "node": "current"
              }
          }]
      ]
    }
  }
}
